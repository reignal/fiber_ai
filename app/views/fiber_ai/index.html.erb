<% unless current_user %>
  <%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
  <%= react_component("FiberAI", { greeting: "Hello, dude or dudette!" }) %>
<% else %>
  <%= current_user[:name] %>
  <%= link_to "Logout", destroy_user_session_path, method: :get %>
  <%= react_component("FiberAI", { greeting: current_user[:name] }) %>
<% end %>